\r\n.btn {\r\n  -moz-appearance: none;\r\n  -webkit-appearance: none;\r\n  appearance: none;\r\n  border: none;\r\n  background: none;\r\n  padding: 0;\r\n  color: black;\r\n  cursor: pointer;\r\n  \r\n  --button-text-color: var(--background-color);\r\n  --button-text-color-hover: var(--button-background-color);\r\n  --border-color: #7D8082;\r\n  --button-background-color: #ece8e1;\r\n  --highlight-color: #005551;\r\n  --button-inner-border-color: transparent;\r\n  --button-bits-color: var(--background-color);\r\n  --button-bits-color-hover: var(--button-background-color);\r\n  \r\n  position: relative;\r\n  padding: 8px;\r\n  margin-bottom: 20px;\r\n  text-transform: uppercase;\r\n  font-weight: bold;\r\n  font-size: 14px;\r\n  transition: all .5s ease;\r\n}\r\n\r\n.btn::before,\r\n.btn::after {\r\n  content: '';\r\n  display: block;\r\n  position: absolute;\r\n  right: 0; left: 0;\r\n  height: calc(50% - 5px);\r\n  border: 1px solid var(--border-color);\r\n  transition: all .5s ease;\r\n}\r\n\r\n.btn::before {\r\n  top: 0;\r\n  border-bottom-width: 0;\r\n}\r\n\r\n.btn::after {\r\n  bottom: 0;\r\n  border-top-width: 0;\r\n}\r\n\r\n.btn:active,\r\n.btn:focus {\r\n  outline: none;\r\n}\r\n\r\n.btn:active::before,\r\n.btn:active::after {\r\n  right: 3px;\r\n  left: 3px;\r\n}\r\n\r\n.btn:active::before {\r\n  top: 3px;\r\n}\r\n\r\n.btn:active::after {\r\n  bottom: 3px;\r\n}\r\n\r\n.btn__inner {\r\n  position: relative;\r\n  display: block;\r\n  padding: 20px 30px;\r\n  background-color: var(--button-background-color);\r\n  overflow: hidden;\r\n  box-shadow: inset 0px 0px 0px 1px var(--button-inner-border-color);\r\n}\r\n\r\n.btn__inner::before {\r\n  content: '';\r\n  display: block;\r\n  position: absolute;\r\n  top: 0; left: 0;\r\n  width: 2px;\r\n  height: 2px;\r\n  background-color: var(--button-bits-color);\r\n}\r\n\r\n.btn__inner::after {\r\n  content: '';\r\n  display: block;\r\n  position: absolute;\r\n  right: 0; bottom: 0;\r\n  width: 4px;\r\n  height: 4px;\r\n  background-color: var(--button-bits-color);\r\n  transition: all .5s ease;\r\n}\r\n\r\n.btn__slide {\r\n  display: block;\r\n  position: absolute;\r\n  top: 0; bottom: -1px; left: -8px;\r\n  width: 0;\r\n  background-color: var(--highlight-color);\r\n  transform: skew(-15deg);\r\n  transition: all .5s ease;\r\n}\r\n\r\n.btn__content {\r\n  position: relative;\r\n}\r\n\r\n.btn__content a {\r\n  color: black;\r\n  text-decoration: none;\r\n  transition: all .5s ease;\r\n}\r\n\r\n.btn:hover .btn__content a{\r\n  color: white;\r\n}\r\n\r\n.btn:hover {\r\n  color: var(--button-text-color-hover);\r\n}\r\n\r\n.btn:hover .btn__slide {\r\n  width: calc(100% + 15px);\r\n}\r\n\r\n.btn:hover .btn__inner::after {\r\n  background-color: var(--button-bits-color-hover);\r\n}\r\n\r\n.btn-middle {\r\n  margin: 0 2rem 0 2rem;\r\n}\r\n\r\n#odor {\r\n  height: 40vh;\r\n  background-color: #ece8e1;\r\n}\r\n\r\n#concern {\r\n  height: 40vh;\r\n  background-color: black;\r\n}\r\n\r\n#story {\r\n  height: 40vh;\r\n  background-color: #005551;\r\n}"},"lineCount":null}},"error":null,"hash":"9bb1668fbd9ce82f06185a1d27064a00","cacheData":{"env":{}}}